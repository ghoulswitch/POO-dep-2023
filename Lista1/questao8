package poolistaum;
import java.util.Scanner;

public class Oito {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a;
		int b;
		double resultado;
		
		System.out.println("Digite o primeiro valor:");
		a = sc.nextInt();
		System.out.println("Digite o segundo valor:");
		b = sc.nextInt();
		
		resultado = (a + b) * (a - b);
		
		System.out.printf("O resultado do quadrado da diferença do primeiro valor pelo segundo é: %.2f", resultado);
		
		sc.close();
	}

}
