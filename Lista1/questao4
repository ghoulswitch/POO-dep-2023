package poolistaum;

public class Quatro {
	
	public static void main(String[] args) {
		
		int val1 = 1;
		int val2 = 2;
		int val3 = 3;
		int val4 = 4;
		double mediaponderada;
		
		mediaponderada = (val1 * 1 + val2 * 2 + val3 * 2 + val4 * 2) / 5;
		
		System.out.printf("A média ponderada é: %.1f", mediaponderada);
	}

}
