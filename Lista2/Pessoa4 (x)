package deppoolista2;

public class Pessoa4 {
	
	String nome;
	char sexo;
	int idade;
	float altura, peso;

	
	public void setPosition(char sexo) {
		this.sexo = sexo;
	}
	
	public char setSexo() {
		return sexo;
	}
	
	public void setSexo(char sexo) {
		this.sexo = sexo;
	}
	
	public void setPosition(float altura) {
		this.altura = altura;
	}
	
	public float setAltura() {
		return altura;
	}
	
	public void setAltura(float altura) {
		this.altura = altura;
	}
	
	

}
